stringify({message})\n    })\n    .then(res => res.json())\n    .then(data => {\n      setResponse(data.message);\n    }\n    )\n\n} \n\n\nreturn (\n  <div className=\"App\">\n    <h1>Bem-vindo ao HenriqueGPT!</h1>\n    <form onSubmit={handleSubmit}>\n      <textarea\n        value={message}\n        placeholder=\"Pergunte ao Henrique o que quiser!\"\n        onChange={e => setMessage(e.target.value)}\n      ></textarea>\n      <button type=\"submit\">Submit</button>\n    </form>\n   {response && <div><b>Henrique:</b>{response}</div>}\n  </div>\n\n);\n\n}\n\nexport default App;\n\n// I love you github Copilot <3\n\n\n",{"ruleId":"20","severity":1,"message":"21","line":3,"column":17,"nodeType":"22","messageId":"23","endLine":3,"endColumn":25},"no-unused-vars","'useState' is defined but never used.","Identifier","unusedVar"]